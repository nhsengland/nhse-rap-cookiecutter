{
    "project_name": "project_name",
    "repo_name": "{{ cookiecutter.project_name.lower().replace(' ', '_') }}",
    "module_name": "{{ cookiecutter.project_name.lower().replace(' ', '_').replace('-', '_') }}",
    "author_name": "Your Name",
    "author_email": "your.email@nhs.net",
    "organization_name": "NHS England",
    "team_name": "Your Team",
    "team_email": "your.team@nhs.net",
    "git_hosting_platform": ["github", "gitlab", "azure_devops", "other"],
    "repository_url": "https://github.com/{{ cookiecutter.organization_name.lower().replace(' ', '') }}/{{ cookiecutter.repo_name }}",
    "description": "A short description of the project.",
    "python_version_number": ["3.10", "3.11", "3.12", "3.13"],
    "environment_manager": [
        "virtualenv",
        "conda",
        "pipenv",
        "uv",
        "pixi",
        "poetry",
        "none"
    ],
    "dependency_file": [
        "requirements.txt",
        "pyproject.toml",
        "environment.yml",
        "Pipfile",
        "pixi.toml"
    ],
    "pydata_packages": [
        "none",
        "basic"
    ],
    "linting_and_formatting": [
        "ruff",
        "flake8+black+isort"
    ],
    "open_source_license": ["MIT", "Apache-2.0", "GPL-3.0", "No license file"],
    "docs": ["mkdocs", "none"],
    "include_code_scaffold": ["Yes", "No"],
    "_copy_without_render": [
        "*.html",
        "*.css",
        "*.js"
    ]
}
